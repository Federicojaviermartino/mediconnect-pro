{
  "name": "mediconnect-pro",
  "version": "1.0.0",
  "description": "Enterprise-grade telemedicine platform with AI-powered risk prediction and remote patient monitoring",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "npm install --production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:verbose": "jest --verbose --coverage",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:postgres": "docker-compose --profile postgres up -d",
    "db:migrate": "node demo-app/database/migrate.js run",
    "db:migrate:status": "node demo-app/database/migrate.js status",
    "db:migrate:rollback": "NODE_ENV=development node demo-app/database/migrate.js rollback",
    "db:setup": "npm run docker:postgres && npm run db:migrate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "pg": "^8.16.3",
    "redis": "^5.8.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Federicojaviermartino/mediconnect-pro.git"
  },
  "keywords": [
    "telemedicine",
    "healthcare",
    "iot",
    "machine-learning",
    "microservices",
    "enterprise",
    "real-time",
    "monitoring",
    "nestjs",
    "nextjs",
    "typescript",
    "fastapi"
  ],
  "author": "Federico <your-email@example.com>",
  "license": "MIT"
}
